<!DOCTYPE html>
<html lang="sv">
<head>
  <meta charset="UTF-8">
  <title>Highscore List</title>
  <style>
    body {
      font-family: Arial, sans-serif;
    }
    h1 {
      color: #333;
    }
    ul {
      list-style: none;
      padding: 0;
    }
    li {
      margin: 10px 0;
      padding: 10px;
      background: #f0f0f0;
      border-radius: 5px;
    }
  </style>
</head>
<body>
  <h1>Highscore List</h1>
  <ul>
    <% highscores.forEach(score => { %>
      <li>
        <strong><%= score.name %></strong> â€“ time: <%= score.time %> ms,
        guesses: <%= score.guesses.join(", ") %>,
        wordLength: <%= score.wordLength %>,
        allowRepeats: <%= score.allowRepeats %>
      </li>
    <% }); %>
  </ul>
</body>
</html>
